{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Parteiparolen",
  "properties": {
    "title": {
      "title": "Titel",
      "type": "string",
      "Q:options": {
        "placeholder": "Der Titel bringt die Kernaussage der Tabelle auf den Punkt."
      }
    },
    "subtitle": {
      "title": "Untertitel",
      "type": "string",
      "Q:options": {
        "placeholder": "Der Untertitel benennt pr√§zise, was in der Grafik zu sehen ist, inklusive Einheiten."
      }
    },
    "data": {
      "type": "object",
      "title": "Daten",
      "properties": {
        "yes": {
          "type": "object",
          "title": "Ja Parole",
          "properties": {
            "parties": {
              "type": "array",
              "title": "Parteien",
              "items": {
                "type": "string",
                "title": "Partei"
              }
            },
            "organisations": {
              "type": "array",
              "title": "Organisationen",
              "items": {
                "type": "string",
                "title": "Organisation"
              }
            }
          }
        },
        "no": {
          "type": "object",
          "title": "Nein Parole",
          "properties": {
            "parties": {
              "type": "array",
              "title": "Parteien",
              "items": {
                "type": "string",
                "title": "Partei"
              }
            },
            "organisations": {
              "type": "array",
              "title": "Organisationen",
              "items": {
                "type": "string",
                "title": "Organisation"
              }
            }
          }
        },
        "undecided": {
          "type": "object",
          "title": "Stimmfreigabe",
          "properties": {
            "parties": {
              "type": "array",
              "title": "Parteien",
              "items": {
                "type": "string",
                "title": "Partei"
              }
            },
            "organisations": {
              "type": "array",
              "title": "Organisationen",
              "items": {
                "type": "string",
                "title": "Organisation"
              }
            }
          }
        }
      }
    }
  },
  "required": [
    "title",
    "data"
  ]
}
